#!/usr/bin/env php
<?php

$rootOfApp = __DIR__ . "/../../../../";

require_once($rootOfApp . "/bootstrap.php");

$scope = APP_CORE_NAME;

$databaseConfigs = $scope::Container()->get("DatabaseConfig");

$zenderator = new \Zenderator\Zenderator(
    $rootOfApp,
    $databaseConfigs
);
$zenderator->makeZenderator();
